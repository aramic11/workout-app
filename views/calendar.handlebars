<h1 class="calendarTitle">Schedule</h1>
<div class="row">
  <table class="table table-bordered">
    <thead>
      <tr>
        <th>Workout</th>
        <th>Set Number</th>
        <th>Rep Amount</th>
        <th>Weight</th>
        <th>Weight Type</th>
        <th>Comments</th>
        <th>Date</th>
        <th>Remove from Schedule</th>
      </tr>
    </thead>
        <tbody>
        {{#each workouts}}
                <tr>
    
                    <td>{{workout.exercise_name}}</td>
                    <td>{{workout.set_number}}</td>
                    <td>{{workout.rep_amount}}</td>
                    <td>{{workout.weight}}</td>
                    <td>{{workout.weight_type}}</td>
                    <td>{{workout.comments}}</td>
                    <td>{{workout.date}}</td>
                    <td><button class="deleteBtn">Remove Workout</button></td>
                    
                </tr>
        {{else}}
        <p>No workouts to display</p>
        {{/each}}
        </tbody>
    </table>
</div>
<script src='/js/calendar.js'></script>